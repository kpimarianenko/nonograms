{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@assets/*": ["./assets/*"],
      "@client": ["./src/client"],
      "@client/*": ["./src/client/*"],
      "@components/*": ["./src/components/*"],
      "@constants/*": ["./src/constants/*"],
      "@gql": ["./src/graphql"],
      "@helpers/*": ["./src/helpers/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@i18n": ["./src/i18n"],
      "@i18n/*": ["./src/i18n/*"],
      "@navigation/*": ["./src/navigation/*"],
      "@screens/*": ["./src/screens/*"],
      "@services/*": ["./src/services/*"],
      "@storage": ["./src/storage"],
      "@theme": ["./src/theme"],
      "@theme/*": ["./src/theme/*"]
    },
    "types": ["node"],
    "target": "es6",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "extends": "@tsconfig/react-native/tsconfig.json",
  "include": ["src", "index.ts", "App.tsx"]
}
